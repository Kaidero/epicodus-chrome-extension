{
  "manifest_version": 2,

  "name": "Epicodus Attendance",
  "description": "Links to the epicodus attendance page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Hi, it's me Spencer."
  },
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*",
    "notifications" 
    ]
}
